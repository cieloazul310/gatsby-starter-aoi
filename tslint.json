{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "interface-name": [ "never-prefix" ],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false
    },
    "rulesDirectory": ["tslint-plugin-prettier"]
}
